{
  "name": "cal-hack",
  "version": "1.0.0",
  "description": "A Duolingo for blind users - accessible language learning platform",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "webpack --mode production",
    "build:prod": "NODE_ENV=production webpack --mode production",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:a11y": "jest --testPathPattern=accessibility",
    "test:accessibility": "playwright test --grep @accessibility",
    "test:screen-reader": "playwright test --grep @screen-reader",
    "test:keyboard": "playwright test --grep @keyboard",
    "test:axe": "axe-core --config axe.config.js",
    "lint": "eslint src/ tests/",
    "lint:fix": "eslint src/ tests/ --fix",
    "report:a11y": "lighthouse-ci --config=lighthouse.config.js",
    "format": "prettier --write src/ tests/",
    "format:check": "prettier --check src/ tests/"
  },
  "keywords": [
    "accessibility",
    "language-learning",
    "blind",
    "screen-reader",
    "inclusive-design",
    "duolingo",
    "a11y"
  ],
  "author": "Cal-Hack Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "axe-core": "^4.8.2",
    "eslint": "^8.54.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "jest": "^29.7.0",
    "jest-axe": "^8.0.0",
    "lighthouse": "^11.4.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.1.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/YOUR_USERNAME/Cal-Hack.git"
  },
  "bugs": {
    "url": "https://github.com/YOUR_USERNAME/Cal-Hack/issues"
  },
  "homepage": "https://github.com/YOUR_USERNAME/Cal-Hack#readme"
}
